#!/bin/bash

set -o nounset
source lib/cmd/*



cloneH5BPgitREPO(){
	ARG="$1"
	git clone https://github.com/h5bp/html5-boilerplate.git $ARG
}
while getopts "h:p:b:n:g:" optionName; do
case "$optionName" in
	h|help) showHelpText;;
	p|php) createPHP $2;;
	b|basic) createHTML $2;;
	n|node) createNODE $2;;
	*) showHelpText;;
esac
shift
done
